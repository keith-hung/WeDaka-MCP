# Test Environment Configuration for WeDaka MCP Server
# Copy this file to .env.test and fill in your actual values

# WeDaka API Server URL (required)
WEDAKA_API_URL=https://your-wedaka-server.com

# Employee Credentials (required for login)
WEDAKA_USERNAME=your_ad_username
WEDAKA_DEVICE_ID=your_device_id

# Employee Number (required for clock in/out)
WEDAKA_EMP_NO=your_employee_number

# Test Settings
TEST_TIMEOUT=30
TEST_VERBOSE=true